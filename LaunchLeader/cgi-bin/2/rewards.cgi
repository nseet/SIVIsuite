{
   $WHERE .= "AND reward_added = $Launchleader::reward->{'reward_added'} ";
}  
if($Launchleader::reward->{'reward_expires'})
{
   $WHERE .= "AND reward_expires = $Launchleader::reward->{'reward_expires'} ";
}  
if($Launchleader::reward->{'reward_type'})
{
   $WHERE .= "AND reward_type = $Launchleader::reward->{'reward_type'} ";
}  
if($Launchleader::reward->{'reward_status'})
{
   $WHERE .= "AND reward_status = $Launchleader::reward->{'reward_status'} ";
}  


return $WHERE;
}
